{
  "name": "ui-landing",
  "version": "1.0.0",
  "description": "Webapp for ms-oauth2-rest",
  "private": true,
  "author": "redbee",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build-server build-client",
    "build-server": "NODE_ENV=production webpack --env.ENVIRONMENT=production --progress --config webpack.server.config.js",
    "build-client": "NODE_ENV=production webpack --env.ENVIRONMENT=production --progress",
    "dev": "npm-run-all --parallel dev-client dev-server",
    "dev-server": "npm-run-all --parallel webpack-server run-server",
    "webpack-server": "NODE_ENV=development webpack --env.ENVIRONMENT=development --progress --watch --config webpack.server.config.js",
    "run-server": "nodemon dist/server.js",
    "dev-client": "webpack --env.ENVIRONMENT=development --watch --progress",
    "bundlesize": "bundlesize",
    "precommit": "eslint src --quiet"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "isomorphic-fetch": "^2.2.1",
    "mysql": "^2.17.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet-async": "^1.0.4",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "swagger-ui": "^3.23.11"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "bundlesize": "^0.17.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^1.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.8.0",
    "mustache-express": "^1.2.8",
    "node-sass": "^4.13.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-livereload-plugin": "^2.2.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
