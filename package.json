{
  "name": "ui-landing",
  "version": "1.0.0",
  "description": "Webapp for ms-oauth2-rest",
  "private": true,
  "author": "redbee",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build-server build-client",
    "build-server": "NODE_ENV=production webpack --env.ENVIRONMENT=production --progress --config webpack.server.config.js",
    "build-client": "NODE_ENV=production webpack --env.ENVIRONMENT=production --progress",
    "dev": "npm-run-all --parallel server client",
    "server": "NODE_ENV=development nodemon index.js",
    "client": "webpack --env.ENVIRONMENT=development --watch --progress",
    "bundlesize": "bundlesize",
    "precommit": "eslint src --quiet"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "setupFiles": [
      "./setupTests.js"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "output/coverage/jest",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.10.0",
    "react": "^16.6.3",
    "react-d3-radar": "^1.0.0-rc4",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "recharts": "^1.4.1",
    "style-loader": "^0.23.1",
    "swagger-ui-react": "^3.23.11"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@material-ui/core": "^4.3.2",
    "axios": "^0.19.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "bundlesize": "^0.17.0",
    "clean-webpack-plugin": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "^1.0.1",
    "dotenv": "^6.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "jquery": "^3.4.1",
    "material-design-icons-iconfont": "^5.0.1",
    "middleware-login-library": "1.1.19",
    "mini-css-extract-plugin": "^0.8.0",
    "mustache-express": "^1.2.8",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "react-bootstrap": "^1.0.0-beta.10",
    "react-switch": "^5.0.1",
    "react-test-renderer": "^16.9.0",
    "sass-loader": "^7.1.0",
    "sonarqube-scanner": "^2.5.0",
    "svg-inline-loader": "^0.8.0",
    "swagger-ui": "^3.23.11",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-livereload-plugin": "^2.2.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
